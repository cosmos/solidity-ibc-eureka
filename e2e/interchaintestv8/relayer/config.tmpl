{
  "server": {
    "log_level": "info",
    "address": "127.0.0.1"
  },
  "modules": [
    {
      "name": "eth_to_cosmos",
      "port": {{ .EthToCosmosPort }},
      "config": {
        "tm_rpc_url": "{{ .TmRPC }}",
        "ics26_address": "{{ .ICS26Address }}",
        "eth_rpc_url": "{{ .EthRPC }}",
        "eth_beacon_api_url": "{{ .BeaconAPI }}",
        "signer_address": "{{ .SignerAddress }}",
        "mock": {{ .MockWasmClient }}
      }
    },
    {
      "name": "cosmos_to_eth",
      "port": {{ .CosmosToEthPort }},
      "config": {
        "tm_rpc_url": "{{ .TmRPC }}",
        "ics26_address": "{{ .ICS26Address }}",
        "eth_rpc_url": "{{ .EthRPC }}",
        "sp1_private_key": "{{ .SP1PrivateKey }}",
        "mock": {{ .MockSP1Client }}
      }
    }
  ]
}
