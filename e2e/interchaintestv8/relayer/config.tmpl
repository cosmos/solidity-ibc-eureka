{
  "modules": [
    {{- range $index, $module := .Modules }}
    {{- if $index }},{{ end }}
    {{ $module.ToJSON }}
    {{- end }}
  ],
  "server": {
    "address": "{{ .Server.Address }}",
    "port": {{ .Server.Port }}
  },
  "observability": {
    "level": "{{ .Observability.Level }}",
    "use_otel": {{ .Observability.UseOtel }},
    "service_name": "{{ .Observability.ServiceName }}"{{ if .Observability.OTelEndpoint }},
    "otel_endpoint": "{{ .Observability.OTelEndpoint }}"{{ end }}
  }
}
